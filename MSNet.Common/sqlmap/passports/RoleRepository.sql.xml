<?xml version="1.0" encoding="utf-8" ?>
<root xmlns="http://m2sa.net/Schema/SqlMapping">
	<module moduleName="RoleRepository" dbName="MSNet.MySql">
    <sqlWrap  sqlName="Insert"  supportDBType="MySql,SqlServer">
      insert into Role(Name,Description,RoleType,CreatedTime,LastModifiedTime)
      values(@Name,@Description,@RoleType,@CreatedTime,@LastModifiedTime);
      select @@IDENTITY;
    </sqlWrap>
    <sqlWrap  sqlName="Update"  supportDBType="MySql,SqlServer">
      update Role  set Name=@Name,Description=@Description,RoleType=@RoleType,LastModifiedTime=@LastModifiedTime
      where RoleId = @Id;
    </sqlWrap>
    <sqlWrap  sqlName="DeleteById"  supportDBType="MySql,SqlServer">
      delete from Role where RoleId = @Id;
    </sqlWrap>
    <sqlWrap  sqlName="SelectById"  supportDBType="MySql,SqlServer">
      select *  from Role  where RoleId = @Id;
    </sqlWrap>
    <sqlWrap  sqlName="SelectAll"  supportDBType="MySql,SqlServer">
      select *  from Role;
    </sqlWrap>
  </module>
</root>

